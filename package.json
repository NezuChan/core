{
  "name": "@nezuchan/core",
  "version": "1.0.0",
  "description": "A TypeScript template project.",
  "main": "dist",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "build": "rimraf dist && tsc && gen-esm-wrapper ./dist/index.js ./dist/index.mjs"
  },
  "files": [
    "dist/**",
    "LICENSE",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "author": "KagChi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@hazmi35/eslint-config": "^8.7.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "gen-esm-wrapper": "^1.1.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/no-extraneous-class": "off"
    }
  },
  "dependencies": {
    "@cordis/bitfield": "^1.2.0",
    "@discordjs/rest": "^1.5.0",
    "@nezuchan/cordis-brokers": "^2.0.0",
    "@nezuchan/redis-collection": "^1.3.0",
    "@sapphire/result": "^2.6.0",
    "@sapphire/snowflake": "^3.4.0",
    "@sapphire/utilities": "^3.11.0",
    "discord-api-types": "^0.37.30",
    "ioredis": "^5.3.0",
    "tslib": "^2.5.0"
  }
}
